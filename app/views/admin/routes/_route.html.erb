      <tr>
        <td>
          <div id = "route_title_<%=route.id%>">
            <%= route.title %>
          </div>
          <%= render 'form', route: route, hidden: true %>
        </td>
        <td><%= link_to t('common.show'), admin_route_path(route) %></td>
        <td><%= link_to t('common.edit'), '', class: 'edit_route', data: { route_id: route.id }  %></td>
        <td><%= link_to t('common.delete'), admin_route_path(route), method: :delete, data: { confirm: t('common.delete_confirm_message') } %></td>
      </tr>
